package exemplo;
import java.util.*; //PUXA TODOS OS METODOS. PRECISA POOR CONTA DO SCANNER




class Somar {
	public static Scanner sc = new Scanner(System.in); //DEFINE O METEDO SC DO TIPO SCANNER
	
	public static void main (String args[]) {
		int num1, num2, num3;
	
		System.out.println("DIGITE UM NUMERO: "); //EQUIVALENTE AO PRINTF
		num1 = sc.nextInt(); //EQUIVALENTE AO SCANF
		System.out.println("DIGITE OUTRO NUMERO: ");
		num2 = sc.nextInt();
		
		num3 = num1 + num2; //REALIZA A SOMA
		
		System.out.println("A SOMA DOS UMEROS E" + num3);
	}
}
